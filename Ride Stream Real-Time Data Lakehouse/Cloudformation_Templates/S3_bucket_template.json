{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "This is the production bucket for Project.",
  "Resources": {
    "MySimpleBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "ridestream-production"
      }
    }
  },
  "Outputs": {
    "BucketName": {
      "Description": "Name of the created S3 bucket",
      "Value": {
        "Ref": "MySimpleBucket"
      }
    }
  }
}
